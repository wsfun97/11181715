<script>
function loadMap ()
{
  var myLatLng = {lat: , lng: };
  //var myLatLng = new google.maps.LatLng({lat: , lng: });
  var map = new google.maps.Map(document.getElementById("map"),{
        zoom: 18,
        center: myLatLng
  });
  var marker = new google.maps.Marker({
     position: myLatLng,
     map: map,
     title: 'k'
  });
}
</script>
